generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model AboutCourse {
  id        String   @id @default(cuid())
  title     String
  info      String?
  content   String   @db.Text
  image     String?
  updatedAt DateTime @updatedAt
}
